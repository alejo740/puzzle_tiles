package data;

class NDiLocalizationData
{

	public var id:String;
	public var fontScale:Float;
	public var offsetX:Float;
	public var offsetY:Float;
	public var fontName:String;
	public var description:String;
	public var content:String;
	
	public function new() 
	{
		this.id = "";
		this.fontScale = 0;
		this.offsetX = 0;
		this.offsetY = 0;
		this.fontName = "";
		this.description = "";
		this.content = "";
	}
}